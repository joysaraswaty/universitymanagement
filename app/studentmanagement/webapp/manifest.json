{
  "_version": "1.82.0",
  "sap.app": {
    "id": "university.school.studentmanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.20.3",
      "toolsId": "6d18db8c-f66a-440d-90cb-04b578723d67"
    },
    "dataSources": {
      "mainService": {
        "uri": "/service/UniversityApp/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.01"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "university-manage": {
          "semanticObject": "university",
          "action": "manage",
          "title": "{{university-manage.flpTitle}}",
          "subTitle": "{{university-manage.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.145.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "university.school.studentmanagement.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "StudentList",
          "target": "StudentList"
        },
        {
          "pattern": "Student({key}):?query:",
          "name": "StudentObjectPage",
          "target": "StudentObjectPage"
        },
        {
          "pattern": "Student({key})/schools({key2}):?query:",
          "name": "SchoolObjectPage",
          "target": "SchoolObjectPage"
        }
      ],
      "targets": {
        "StudentList": {
          "type": "Component",
          "id": "StudentList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Student",
              "variantManagement": "Page",
              "navigation": {
                "Student": {
                  "detail": {
                    "route": "StudentObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              }
            }
          }
        },
        "StudentObjectPage": {
          "type": "Component",
          "id": "StudentObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Student",
              "navigation": {
                "schools": {
                  "detail": {
                    "route": "SchoolObjectPage"
                  }
                }
              }
            }
          }
        },
        "SchoolObjectPage": {
          "type": "Component",
          "id": "SchoolObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Student/schools"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.fe": {
    "app": {
      "enableLazyLoading": true
    }
  }
}
